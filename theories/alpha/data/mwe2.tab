- %intnum	%concat(%intnum)
%hour - %hour	%concat(%hour-interval)[1;3]
%intnum - latek(subst:$n:$c:$g)	x-latek(subst:$n:$c:$g)[1]
%intnum - tka(subst:$n:$c:$g)	x-tka(subst:$n:$c:$g)[1]
